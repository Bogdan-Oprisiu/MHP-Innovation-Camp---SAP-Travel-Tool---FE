{
  "_version": "1.59.0",
  "sap.app": {
    "id": "bts.btsapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.2",
      "toolsId": "6fa51cee-2f8c-4227-bd18-d4f209e3276b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZMHP_BUSINESS_TRIPS_SRV/",
        "type": "OData",
        "settings": {
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.126.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "bts.btsapp.i18n.i18n"
        }
      },
      "mockUserData": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/mock_user_data.json"
      },
      "mainServiceModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "mainService",
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "useBatch": true
        },
        "mockBTs": {
          "type": "sap.ui.model.json.JSONModel",
          "uri": "model/MockBTs.json"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "bts.btsapp.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteWelcome",
          "pattern": ":?query:",
          "target": ["TargetWelcome"]
        },
        {
          "name": "RouteTest",
          "pattern": "test",
          "target": ["TargetTest"]
        }
      ],
      "targets": {
        "TargetWelcome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Welcome",
          "viewName": "Welcome"
        },
        "TargetDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Details",
          "viewName": "Details"
          },
        "TargetTest": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Test",
          "viewName": "Test"
        }
      }
    },
    "rootView": {
      "viewName": "bts.btsapp.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
